[MASTER]

persistent=no
extension-pkg-whitelist=

[MESSAGES CONTROL]

disable=too-few-public-methods,too-many-public-methods,too-many-instance-attributes,maybe-no-member,star-args

[TYPECHECK]

ignored-modules=gi.repository.Gtk,gi.repository.Gdk,gtk,cairo
ignored-classes=
generated-members=

[BASIC]

good-names=i,j,k,x,y,z,db,fp,ex,Run,_,gtk_image_new_from_file,gtk_image_new_from_stock,cairo_set_source_pixbuf,rsvg_handle_new_from_file,rsvg,izip,xrange,up,UP,TimeVal

[DESIGN]

max-parents=8

[FORMAT]

max-line-length=80

[VARIABLES]

dummy-variables-rgx=^(args|kw|ev|)_$
